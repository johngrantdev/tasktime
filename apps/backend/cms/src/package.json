{
  "name": "johngrant-dev",
  "description": "Website template for Payload",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts nodemon",
    "seed": "rm -rf media && cross-env PAYLOAD_SEED=true PAYLOAD_DROP_DATABASE=true PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts ts-node src/server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build:next:skipssg": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NEXT_BUILD=true next experimental-compile",
    "build": "cross-env NODE_ENV=production pnpm build:payload && pnpm build:server && pnpm copyfiles && pnpm build:next",
    "build:skipssg": "cross-env NODE_ENV=production pnpm build:payload && pnpm build:server && pnpm copyfiles && pnpm build:next:skipssg",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,js}\" dist/",
    "generate:nextssg": "cross-env PAYLOAD_CONFIG_PATH=dist/payload/payload.config.ts next experimental-generate",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload/payload.config.ts payload"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.529.1",
    "@aws-sdk/lib-storage": "^3.529.1",
    "@innovixx/payload-color-picker-field": "^1.1.0",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-postgres": "^0.7.0",
    "@payloadcms/plugin-cloud-storage": "^1.1.1",
    "@payloadcms/plugin-form-builder": "^1.2.1",
    "@payloadcms/plugin-nested-docs": "^1.0.12",
    "@payloadcms/plugin-redirects": "^1.0.1",
    "@payloadcms/plugin-search": "^1.1.0",
    "@payloadcms/plugin-seo": "^2.2.1",
    "@payloadcms/richtext-lexical": "^0.7.0",
    "@react-spring/web": "^9.7.3",
    "aws-crt": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "escape-html": "^1.0.3",
    "express": "^4.18.3",
    "lexical": "^0.13.1",
    "next": "14.1.3",
    "next-themes": "^0.2.1",
    "payload": "^2.11.2",
    "payload-blurhash-plugin": "^2.0.0",
    "prism-react-renderer": "^2.3.1",
    "qs": "6.12.0",
    "react": "^18.2.0",
    "react-code-blocks": "^0.1.5",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-player": "^2.15.1",
    "react-router-dom": "6.22.3",
    "type-fest": "^4.12.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.1.3",
    "@payloadcms/eslint-config": "^1.1.1",
    "@swc/core": "^1.4.6",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.9",
    "@types/node": "20.11.26",
    "@types/qs": "^6.9.12",
    "@types/react": "18.2.64",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "autoprefixer": "^10.4.18",
    "copyfiles": "^2.4.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.31",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.5",
    "tailwindcss-3d": "^1.0.5",
    "ts-node": "^10.9.1",
    "typescript": "5.4.2"
  }
}
