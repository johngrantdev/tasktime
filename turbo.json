{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
      "dev": {
        "cache": false,
        "persistent": true
      },
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "build/**", ".next/**", "!.next/cache/**"]
      },
      "lint": {
        "dependsOn": ["^lint"]
      }
    }
  }